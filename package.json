{
  "name": "clockapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev": "npx expo install expo-dev-client && npx expo run:android --variant debug",
    "build:release": "npx expo run:android --variant release",
    "install:apk": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "device:check": "adb devices"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/native": "^7.1.10",
    "expo": "~53.0.11",
    "expo-device": "^7.1.4",
    "expo-keep-awake": "^14.1.4",
    "expo-notifications": "^0.31.3",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "expo-dev-client": "~5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
